<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>

    <ui:with field='msg'
             type='com.btxtech.game.jsre.client.ClientI18nConstants'/>

    <!-- g:HTMLPanel -->
		<g:MenuBar>
			<g:MenuItem text="{msg.register}" ui:field="registerSubMenuBar"  title="{msg.menuTooltipRegisterLogin}">
				<g:MenuBar vertical="true">
					<g:MenuItem text="{msg.register}" ui:field="register" />
					<g:MenuItem text="{msg.login}" ui:field="login" />
				</g:MenuBar>
			</g:MenuItem>
			<g:MenuItem text="{msg.logout}" ui:field="logout"/>
            <g:MenuItem text="{msg.newBase}" ui:field="newBase" title="{msg.menuTooltipNewBase}"/>
            <g:MenuItem text="{msg.menuNews}" ui:field="news" title="{msg.menuTooltipNews}"/>
			<g:MenuItem text="{msg.menuHistory}" ui:field="history" title="{msg.menuTooltipHistory}" />
		</g:MenuBar>
	<!-- /g:HTMLPanel -->
</ui:UiBinder> 
