<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
    <ui:style>
        .simplepager {
            background: #FFFFFF;
            border-top: 2px solid #6f7277;
        }
    </ui:style>
    <g:VerticalPanel>
        <g:Cell horizontalAlignment="ALIGN_CENTER">
            <g:SimplePanel width="40em" height="45em">
                <p1:CellTable ui:field="cellTable" width="100%" height="100%"/>
            </g:SimplePanel>
        </g:Cell>
        <g:Cell horizontalAlignment="ALIGN_CENTER">
            <g:SimplePanel styleName="{style.simplepager}">
                <p1:SimplePager location="CENTER" ui:field="simplePager"/>
            </g:SimplePanel>
        </g:Cell>
        <g:Label ui:field="notRegisteredLabel"/>
    </g:VerticalPanel>
</ui:UiBinder> 