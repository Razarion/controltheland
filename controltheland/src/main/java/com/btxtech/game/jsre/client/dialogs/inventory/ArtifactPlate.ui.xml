<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.statustext {
            color: black;
		    font-size: 11px;
			font-family: Arial,Helvetica,sans-serif;
			font-weight: bold;
            text-shadow: 1px 1px 1px #000000;
			z-index: 2;
		}
		.imagestyle {
			z-index: 1;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel width="100px" height="100px" ui:field="panel">
			<g:at left="0" top="0">
				<g:Label text="???" ui:field="label" styleName="{style.statustext}"/>
			</g:at>
			<g:at left="0" top="0">
				<g:Image ui:field="image" styleName="{style.imagestyle}"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 