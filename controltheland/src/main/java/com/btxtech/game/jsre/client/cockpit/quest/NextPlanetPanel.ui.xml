<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	
	<ui:with field='res'
		type='com.btxtech.game.jsre.client.cockpit.quest.images.QuestVisualisationImageBundle' />
	
	<g:VerticalPanel>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="Proceed to the next planet" />
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:PushButton ui:field="startButton" title="Move to the next planet!">
			<g:upFace image='{res.startmissionUp}' />
			<g:downFace image='{res.startmissionUp}' />
		</g:PushButton>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 