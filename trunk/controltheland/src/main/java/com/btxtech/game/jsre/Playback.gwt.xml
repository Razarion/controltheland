<module rename-to='playback'>
    <source path="playback"/>
    <source path="client"/>
    <source path="common"/>

    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <inherits name="com.google.gwt.widgetideas.HyperlinkOverride"/>
    <inherits name='com.google.gwt.widgetideas.GWTCanvas'/>
    <inherits name='com.allen_sauer.gwt.voices.gwt-voices'/>
    <inherits name='com.google.gwt.widgetideas.GlassPanel'/>
    <!-- <inherits name='com.google.gwt.base.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.base.theme.dark.Dark'/>     -->

    <!-- Other module inherits                                      -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.btxtech.game.jsre.playback.PlaybackEntry'/>
</module>
