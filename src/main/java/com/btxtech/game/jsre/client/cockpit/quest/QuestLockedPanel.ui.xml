<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	    .text {
            color: #C7C4BB;
            font-size: 12px;
            font-family: Arial, Helvetica, sans-serif;
        }
	</ui:style>
	
	 <ui:with field='msg'
         type='com.btxtech.game.jsre.client.ClientI18nConstants'/>
		
	<g:VerticalPanel spacing="10">
		<g:Label text="{msg.questLocked}" styleName="{style.text}"/>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Button text="{msg.unlockButton}" ui:field="button" styleName="singleButton"/>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 